(define (day-of-week day month year)
  (remainder (- (+ day (- year (quotient (- 14 month) 12)) (quotient (* 31 (- (+ month (* 12 (quotient (- 14 month) 12))) 2)) 12)
                   (quotient (- year (quotient (- 14 month) 12)) 4)(quotient (- year (quotient (- 14 month) 12)) 400))
                (quotient (- year (quotient (- 14 month) 12)) 100))
             7 )
  )
(day-of-week 04 12 1975) 
(day-of-week 04 12 2006) 
(day-of-week 29 05 2013)